{
  "Console Log": {
    "prefix": "cl",
    "body": "console.log($1);",
    "description": "Console Log"
  },
  "Named Function": {
    "prefix": "fnn",
    "body": [
      "function ${1}() {",
      "  $2",
      "}"
    ],
    "description": "Named Function"
  },
  "Anonymous Function": {
    "prefix": "fna",
    "body": [
      "function() {",
      "  $2",
      "}"
    ],
    "description": "Anonymous Function"
  },
  "fori": {
    "prefix": "fori",
    "body": [
      "for (int ${1:i} = 0; $1 < ${2:}; $1++) {",
      "    $0",
      "}"
    ],
    "description": "Anonymous Function"
  },
  "while": {
    "prefix": "while",
    "body": [
      "while ($1)"
    ],
    "description": "while"
  }
}
